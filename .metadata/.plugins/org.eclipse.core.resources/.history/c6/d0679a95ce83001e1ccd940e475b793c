package com.skillstorm.typecast;

import java.util.Scanner;

public class FunWChars {
	
	public static void main (String[] args) {
		
		Scanner decScan = new Scanner(System.in); 
		
		System.out.println("Provide your encoded message: ");
		
		
		
		String encMessage = decScan.next();
		
		char first = (char) (encMessage.charAt(0) - 1);
		char second = (char) (encMessage.charAt(0) - 3);
		char third = (char) (encMessage.charAt(0) - 5);
		char fourth = (char) (encMessage.charAt(0) - 7);
		char fifth = (char) (encMessage.charAt(0) - 9);
		char sixth = (char) (encMessage.charAt(0) - 11);
		char seventh = (char) (encMessage.charAt(0) - 13);
		char eighth = (char) (encMessage.charAt(0) - 15);
		char ninth = (char) (encMessage.charAt(0) - 17);
		char tenth = (char) (encMessage.charAt(0) - 19);
		
		decScan.close();
	}
}
