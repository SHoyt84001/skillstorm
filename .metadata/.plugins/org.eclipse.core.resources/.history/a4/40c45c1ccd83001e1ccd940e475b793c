package com.skillstorm.typecast;

public class FunWChars {
	
	public static void main (String[] args) {
		
		int eRotated = 'e' +2;
		int rRotated = 'r' +2;
		int iRotated = 'i' +2;
		int cRotated = 'c' +2;
		int aRotated = 'a' +2;
		
		System.out.println(" " + (char) eRotated +  (char) rRotated + (char) iRotated + (char) cRotated + (char) aRotated);
	}
}
