package com.skillstorm.typecast;

public class typeCast {

	public static void main (String[] args) {
		
		int num1 = 10; 
		double num2 = 1239.1481; 
		int data2 = (int)num2; 
		double data1 = num1;
		
		System.out.println("This is the int: " + data1);
		
		System.out.println("This in the double: " + data2);
		
		
	}
}